{
  "name": "ruta",
  "description": "Route-Value Collections",
  "author": {
    "name": "Alexander Kit",
    "email": "alex.kit@atmajs.com"
  },
  "version": "0.2.31",
  "main": "./lib/ruta.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/atmajs/Ruta.git"
  },
  "devDependencies": {
    "atma": ">0.8.0",
    "atma-loader-babel": "^1.1.28",
    "babel-preset-es2015": "^6.22.0"
  },
  "scripts": {
    "test": "atma test"
  },
  "license": "MIT",
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "test",
          "es6"
        ],
        "babel": {
          "presets": [
            "es2015"
          ]
        }
      }
    }
  }
}
