{
    "name": "ruta",
    "description": "Route-Value Collections",
    "author": {
        "name": "Alexander Kit",
        "email": "alex.kit@atmajs.com"
    },
    "version": "0.2.40",
    "main": "./lib/ruta.js",
    "types": "./lib/ruta.d.ts",
    "repository": {
        "type": "git",
        "url": "git://github.com/atmajs/Ruta.git"
    },
    "devDependencies": {
        "@types/sinon": "^2.3.3",
        "app-bundler": "0.0.33",
        "atma": "^0.10.81",
        "atma-loader-ts": "^1.0.0",
        "dts-bundle": "^0.7.3"
    },
    "scripts": {
        "test": "atma test",
        "build": "app-bundler && npm run build-dts",
        "server": "atma server",
        "build-dts": "tsc -p . & atma custom tools/build-dts"
    },
    "license": "MIT",
    "app-bundler": {
        "file": "src/export.ts",
        "outputMain": "lib/ruta.js",
        "package": {
            "module": "commonjs",
            "moduleName": "ruta",
            "moduleWrapper": "umd",
            "commonjs": {
                "output": "simplified"
            }
        },
        "extensions": {
            "io": {
                "type": "js"
            }
        },
        "defaultExtensions": {
            "js": "ts"
        },
        "middlewares": {
            "js": [
                "importer:read"
            ],
            "ts": [
                "importer:read",
                "atma-loader-ts:read"
            ]
        }
    },
    "atma": {
        "plugins": [
            "atma-loader-ts"
        ],
        "settings": {
            "atma-loader-ts": {
                "extensions": [
                    "test",
                    "ts"
                ],
                "typescript": {
                    "compilerOptions": {
                        "module": "amd"
                    }
                }
            }
        }
    },
    "dependencies": {
        "@types/jquery": "^3.2.12"
    }
}
